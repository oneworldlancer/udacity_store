{
  "name": "fwd_store_1101",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prettier-f": "prettier --write src/**/*.{ts,js,json,css,html}",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "jasmine": "jasmine",
    "nodemon": "nodemon ./src/index.ts --watch",
    "app-build": "npx tsc",
    "app-start": "nodemon ./src/index.ts && npm run app-build",
    "app-test": "npm run app-build && npm run jasmine",
    "app-clean": "rimraf build/",
    "app-migrate-up-dev": "db-migrate --env dev up",
    "app-migrate-up-tst": "db-migrate --env tst up",
    "app-migrate-down-dev": "db-migrate --env dev down",
    "app-migrate-down-tst": "db-migrate --env tst down",
    "app-migrate-reset-dev": "db-migrate --env dev reset",
    "app-migrate-reset-tst": "db-migrate --env tst reset",
    "app-test-dev": "set iNODE_ENV=dev && tsc && db-migrate up --env dev && tsc && jasmine",
    "app-test-tst": "set iNODE_ENV=tst && tsc && db-migrate up --env tst && tsc && jasmine",
    "app-db-reset-tst": "set iNODE_ENV=tst && db-migrate reset --env tst",
    "app-test-tst1": "set iNODE_ENV=tst && tsc && jasmine"
  },
